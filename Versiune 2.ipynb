+def _crossover(a: pd.Series, b: pd.Series) -> pd.Series:
+    a1 = a.shift(1); b1 = b.shift(1)
+    return (a1 <= b1) & (a > b)
+
+def _crossunder(a: pd.Series, b: pd.Series) -> pd.Series:
+    a1 = a.shift(1); b1 = b.shift(1)
+    return (a1 >= b1) & (a < b)
+
